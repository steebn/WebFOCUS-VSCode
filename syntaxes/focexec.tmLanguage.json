{
	"name": "focexec",
	"scopeName": "source.fex",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#strings"
		},
		{
			"include":"#numbers"
		},
		{
			"include":"#htmlform"
		},
		{
			"include": "#properties"
		},
		{
			"include": "#engines"
		},
		{
			"include":"#dialoguemgr"
		},
		{
			"include": "#variables"
		},
		{
			"include":"#reporting"
		},
		{
			"include": "#dialoguecond"
		},
		{
			"include":"#conditions"
		},
		{
			"include":"#modify"
		}
	],
	"repository": {
		"dialoguemgr" :{
			"begin": "(-(TYPE|RUN|INCLUDE|EXIT))\\b",
			"beginCaptures": {
				"0": {
					"name": "keyword.other.define.focexec"
				}
			},
			"end": "$",
			"endCaptures": {
				"0": {
					"name": "punctuation.section.property-list.end.bracket.curly.css"
				}
			},
			"name": "meta.property-list.css",
			"patterns": [
				{
					"include":"#variables"
				}
			]
		},
		"dialoguecond": {
			"begin": "^(-)(READ|GOTO|CLOSE|PASS|READ|READFILE|WRITE|\\?|START|SET|IF|DEFAULT|DEFAULTS|DEFAULTH|REPEAT)*\\b",
			"beginCaptures": {
				"0": {
					"name": "keyword.other.define.focexec"
				}
			},
			"end": "$",
			"endCaptures": {
				"0": {
					"name": "punctuation.section.property-list.end.bracket.curly.css"
				}
			},
			"name": "meta.property-list.css",
			"patterns": [
				{
					"include":"#variables"
				},
				{
					"include":"#conditions"
				},
				{
					"include": "#strings"
				},
				{
					"include":"#numbers"
				}
			]
		},
		"formats" : {
			"patterns":[
				{
					"match":"\\b(A[0-9]{1,}(B|V)*)\\b",
					"name": "constant.numeric.focexec"
				},
				{
					"match":"\\b(TX[0-9]*)\\b",
					"name": "constant.numeric.focexec"
				},
				{
					"match":"\\b(I[0-9]{1,}(C|S)*)\\b",
					"name": "constant.numeric.focexec"
				},
				{
					"match":"\\b((D|P|F)[0-9]{1,}[.0-9]*(S|M|N|B|C)*)\\b",
					"name": "constant.numeric.focexec"
				},
				{
					"match":"\\b(DATE|YYMD|MDYY|YMD|A4MD|HYYMDs|HYYMDm|HHISsm|A8YYMD|I8YYMD|A8MDYY|I8MDYY|I6MDY|I6YYM|I6YMTD|I6YMD|QYY|MYY|AUTODATE)\\b",
					"name": "constant.numeric.focexec"
				},
				{
					"match":"\\b((S|SH)[0-9]{1,})\\b",
					"name": "keyword.control.define.focexec"
				},
				{
					"match":"=",
					"name": "punctuation.separator.focexec" 
				}
			]
		},
		"modify": {
			"patterns": [
				{
					"match": "\\b(MODIFY|FILE|DATA|FILE|FIXFORM|FREEFORM|ON|FROM|MATCH|NOMATCH|DATA)\\b",
					"name": "keyword.other.define.focexec"
				},
				{
					"match": "\\b(REJECT|INCLUDE|CONTINUE|UPDATE)\\b",
					"name": "keyword.control.define.focexec"
				}
			]
		},
		"variables": {
			"patterns": [
				{
					"match": "(?<!&)&{1,2}\\b(\\w+)\\b",
					"name":"variable.name.other.focexec"
				},	
				{
					"match": "(\\.)(EXISTS|TYPE|LENGTH|EVAL)\\b",
					"name" : "support.function.expression.focexec"
				}
			]
		},
		"htmlform": {
			"begin" : "^-(HTMLFORM)\\b[\\s\\t\\n]{1,}\\b(BEGIN)\\b",
			"beginCaptures": {
				"0": {
					"name": "keyword.other.define.focexec"
				},
				"1": {
					"name": "keyword.other.define.focexec"
				}
			},
			"end" : "^-(HTMLFORM)\\b[\\s\\t\\n]{1,}\\b(END)\\b",
			"endCaptures": {
				"0": {
					"name": "keyword.other.define.focexec"
				}
			},
			"name":"meta.function.define.focexec",
			"patterns": [
				{
					"include": "#comments"
				},
				{
					"include": "#variables"
				},
				{
					"include": "#strings"
				},
				{
					"include":"#numbers"
				}
			]
		},
		"conditions":{
			"patterns":[
				{
					"match": "\\b(GOTO|TIMES|FOR|FROM|TO|STEP)\\b",
					"name": "keyword.other.comparison.focexec"
				},	
				{
					"match": "\\b(IF|ELSE|WHERE|WHEN|THEN)\\b",
					"name": "keyword.other.comparison.focexec"
				},
				{
					"match": "\\b(EQ|NE|LE|LT|GE|GT|IN|IS|NOT|CONTAINS|LIKE|OR|AND)\\b",
					"name": "keyword.control.comparison.focexec"
				},
				{
					"match": "\\*",
					"name": "keyword.other.unit.math.focexec"
				},
				{
					"match": "[!<>]?=|<>|<|>",
					"name": "keyword.control.comparison.focexec"
				},
				{
					"match": "(-|\\+|/)",
					"name": "keyword.other.unit.math.focexec"
				},
				{
					"match": "\\|",
					"name": "keyword.control.comparison.focexec"
				},
				{
					"match": "\\|\\|",
					"name": "keyword.control.comparison.focexec"
				},
				{
					"include": "#formats"
				},
				{
					"include": "#functions"
				},
				{
					"include": "#strings"
				},
